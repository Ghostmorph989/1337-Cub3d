#include <string.h>
#include <stdio.h>

char array[1000];
char Checked[1000];
char dis[1000];

int    ft_checked(char c)
{
    int i;

    i = 0;
    while (Checked[i] != '\0')
    {
        if (Checked[i] == c)
            return (0);
        i++;
    }
    return (1);
}
int     already_printed(char c)
{
    static int i;
    int j;
    static int k;

    j = 0;
    array[i] = c;
    i++;
    while (array[j] != '\0')
    {
        if (array[j] == c && ft_checked(array[j]))
        {
            Checked[k] = c; 
            return (0);
        }
        j++;
    }
    return (1);
}

int     ft_displayed(char c)
{
    int i;

    i = 0;
    while (dis[i] != '\0')
    {
        if (dis[i] == c)
            return (0);
        i++;
    }
    return (1);
}

void    ft_inter(char *Displayed, char *Check_double)
{
    int i;
    int j;
    static int l;

    i = 0;
    j = 0;
    while (Displayed[i] != '\0')
    {
        j = 0;
        while (Check_double[j] != '\0')
        {
            if (Displayed[i] == Check_double[j] && !already_printed(Displayed[i]))
            {
                if (ft_displayed(Displayed[i]))
                    printf("%c", Displayed[i]);
                dis[l] = Displayed[i];
                l++;
            }
            j++;
        }
        i++;
    }
   //printf("\n");
}


int     main(int argc, char **argv)
{
    ft_inter(argv[1], argv[2]);
    return (0);
}