#include <stdio.h>

char printed[1000];

int ft_printed(char c)
{
    int i;

    i = 0;
    while (printed[i] != '\0')
    {
        if (c == printed[i])
            return (1);
        i++;
    }
    return (0);
}

void    ft_union(char *s1, char *s2)
{
    int i;
    int j;
    static int k;

    i = 0;
    j = 0;
    while (s1[i] != '\0')
    {
        if (!ft_printed(s1[i]))
        {
            printed[k] = s1[i];
            k++;
            printf("%c", s1[i]);
        }
        i++;
    }
    i = 0;
    while (s2[i] != '\0')
    {
        if (!ft_printed(s2[i]))
        {
            printed[k] = s2[i];
            k++;
            printf("%c", s2[i]);
        }
        i++;
    }
}

int     main(int argc, char **argv)
{
    if (argc == 3)
        ft_union(argv[1], argv[2]);
    else
        printf("\n");
    return (0);
}
